{
    "name": "pingencom/pingen2-sdk-php",
    "type": "library",
    "license": "BSD-3-Clause",
    "description": "Official PHP Pingen SDK for API V2",
    "require": {
        "php": "^7.4",
        "ext-json": "*",
        "league/oauth2-client": "^2.6",
        "illuminate/support": "^8.17",
        "illuminate/http": "^8.17",
        "spatie/data-transfer-object": "^2.6"
    },
    "autoload": {
        "psr-4": {
            "Pingen\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Pingen GmbH",
            "email": "info@pingen.com"
        }
    ],
    "require-dev": {
        "php-parallel-lint/php-parallel-lint": "^1.2",
        "phpstan/phpstan": "0.12.42",
        "nikic/php-parser": "4.9.*",
        "phpunit/phpunit": "^9.3",
        "rector/rector": "0.8.7",
        "mockery/mockery": "^1.4",
        "symplify/easy-coding-standard-prefixed": "9.2.6"
    }
}
